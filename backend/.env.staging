# Feature Flags Configuration

# DSPy Delegation Optimization
USE_DSPY_DELEGATION=false  # Set to 'true' to enable DSPy
DSPY_ROLLOUT_PERCENTAGE=0  # Gradual rollout: 0-100%
USE_OPTIMIZED_DSPY=true    # Use optimized vs base module
ENABLE_DSPY_TRAINING=false # Enable training mode

# Delegation Logging
LOG_DELEGATION_DECISIONS=true  # Log all decisions for training

# Staging Environment
ENVIRONMENT=staging

# OpenAI API (for DSPy)
OPENAI_API_KEY=your-openai-api-key-here

# Temporal Configuration
TEMPORAL_HOST=localhost:7233
TEMPORAL_NAMESPACE=ve-saas

# Supabase
SUPABASE_URL=your-supabase-url
SUPABASE_SERVICE_KEY=your-service-key

# Redis
REDIS_URL=redis://localhost:6379

# OpenObserve
OPENOBSERVE_OTLP_ENDPOINT=http://localhost:5080
OPENOBSERVE_AUTH_TOKEN=your-auth-token
OPENOBSERVE_ORG=default

# Agent Gateway
AGENT_GATEWAY_URL=http://localhost:8080

# Centrifugo
CENTRIFUGO_API_URL=http://localhost:8000/api
CENTRIFUGO_API_KEY=your-api-key
